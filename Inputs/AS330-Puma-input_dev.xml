<PropertyList>

    <keyboard>

			<key n="69">
			    <name>E</name>
			    <desc>Toggle Aircraft Rendering Effect.</desc>
			    <binding>
				<command>property-toggle</command>
				<property>/sim/model/rendering/shader-effects</property>
			    </binding>
			</key>
                        <key n="123">
                            <name>{</name>
                            <desc>CutOff</desc>                            
			    <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/engine/left-engine</property>
				<value>1</value>
			    </binding>
			    <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/engine/right-engine</property>
				<value>1</value>
			    </binding>
			    <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/engine/left-cutoff</property>
				<value>0</value>
			    </binding>
			    <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/systems/engine/right-cutoff</property>
				<value>0</value>
			    </binding>
			    <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/propulsion/engine[0]/cutoff</property>
				<value>0</value>
			    </binding>			    
			</key>
                        <key n="125">
                            <name>}</name>
                            <desc>Toggle Electric power</desc>
			    <binding>
				<command>property-toggle</command>
				<property>fdm/jsbsim/electrical/switch/battery</property>
			    </binding>
			    <binding>
				<command>property-toggle</command>
				<property>fdm/jsbsim/electrical/switch/alternator</property>
			    </binding>
			    <binding>
				<command>property-toggle</command>
				<property>fdm/jsbsim/animation/electrical-master-switch</property>			
			    </binding>
                            <binding>
                                <command>property-toggle</command>
				<property>fdm/jsbsim/animation/master-switch-cmd</property>
                            </binding>
			     <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/animation/master-switch-lock-cmd</property>
				<value>1</value>
			    </binding>
			</key>
			<key n="115">
                            <name>s</name>
                            <desc>start both engines</desc>
                            <binding>
                                <command>property-assign</command>
				<property>fdm/jsbsim/systems/engine/left-starter</property>
                                <value>1</value>
                            </binding>
			    <binding>
                                <command>property-assign</command>
				<property>fdm/jsbsim/systems/engine/right-starter</property>
                                <value>1</value>
                            </binding>
                        </key>
			<key n="102">
                            <name>f</name>
                            <desc>Blades extended.</desc>
                            <binding>
                                <command>property-assign</command>
				<property>fdm/jsbsim/fcs/wing-fold-cmd-norm</property>
                                <value>0</value>
                            </binding>
                        </key>
                        <key n="70">
                            <name>F</name>
                            <desc>Blades  folded.</desc>
                            <binding>
                                <command>property-assign</command>
                                <property>fdm/jsbsim/fcs/wing-fold-cmd-norm</property>
                                <value>1</value>
                            </binding>
                            <binding>
                                <command>property-assign</command>
                                <property>fdm/jsbsim/animation/rotor_break-switch-cmd</property>	
                                <value>1</value>
                            </binding>
			     <binding>
                                <command>property-assign</command>
                                <property>fdm/jsbsim/animation/rotor_break-switch-lock-cmd</property>
                                <value>1</value>
                            </binding>
                        </key>
                        <key n="100">
                                <name>d</name>
                                <desc>Toggle cockpit Left-Right Door</desc>
                                <binding>
                                        <command>property-toggle</command>
					<property>fdm/jsbsim/animation/cockpit-left-door-pos-cmd</property>
                                </binding>
				<binding>
                                        <command>property-toggle</command>
					<property>fdm/jsbsim/animation/cockpit-right-door-pos-cmd</property>
                                </binding>
                        </key>

                        <key n="68">
                            <name>D</name>
                            <desc>Toggle cabin Left-Right Door</desc>
                            <binding>
                                <command>property-toggle</command>
				<property>fdm/jsbsim/animation/left-cabin_door-pos-cmd</property>
                            </binding>
                            <binding>
                                <command>property-toggle</command>
				<property>fdm/jsbsim/animation/right-cabin_door-pos-cmd</property>
                            </binding>
                        </key>

                        <key n="107">
                            <name>k</name>
                            <desc>Toggle rotor brake</desc>
                             <binding>
				<command>property-toggle</command>
				<property>fdm/jsbsim/animation/rotor_brake-switch-cmd</property>
			    </binding>
			    <binding>
				<command>property-toggle</command>
				<property>fdm/jsbsim/animation/rotor_brake-switch-lock-cmd</property>
			    </binding>
			    <binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/fcs/wing-fold-cmd</property>
				<value>0</value>
			    </binding>
                        </key>
			<key n="75">
                            <name>K</name>
                            <desc>Toggle rotor Clutch</desc>
                            <binding>
                                <command>property-toggle</command>
                                <property>fdm/jsbsim/systems/rotor/clutch-cmd-norm</property>
                            </binding>
                        </key>
                        <key n="117">
                            <name>u</name>
                            <desc>Toggle autothrottle</desc>
                            <binding>
                                <command>property-toggle</command>
                                <property>fdm/jsbsim/fcs/rpm-governor-active-norm</property>
                            </binding>
                        </key>
                        <key n="106">
                            <name>j</name>
                            <desc>Jump-Signal</desc>
                            <binding>
                                <command>property-toggle</command>
                                <property>/controls/jump-signal</property>
                            </binding>
                        </key>

                        <key n="114">
                            <name>r</name>
                            <desc>Toggle Rescue</desc>
                            <binding>
                                <command>property-toggle</command>
                                <property>/sim/model/rescue</property>
                            </binding>
                        </key>
                        <key n="119">
                            <name>w</name>
                            <desc>Lift Winch</desc>
                            <repeatable type="bool">true</repeatable>
                            <binding>
                                <command>property-adjust</command>
                                <property>/sim/model/rescue-lift</property>
                                <step type="double">-0.01</step>
                                <min>0.0</min>
                                <max>1</max>
                            </binding>
                        </key>
                        <key n="87">
                            <name>W</name>
                            <desc>Down Winch</desc>
                            <repeatable type="bool">true</repeatable>
                            <binding>
                                <command>property-adjust</command>
                                <property>/sim/model/rescue-lift</property>
                                <step type="double">0.01</step>
                                <min>0.0</min>
                                <max>1</max>
                            </binding>
                        </key>
		</keyboard>
</PropertyList>