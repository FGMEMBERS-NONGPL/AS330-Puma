        </offsets>
    </model>
    
    <model>
        <name>Light-cons42</name>
	<path>Aircraft/AS330-Puma/Models/Remb/Light_cons.xml</path>
        <offsets>
            <x-m>-0.2</x-m>
            <y-m>0.25</y-m>
            <z-m>-0.02</z-m>	
	    <pitch-deg>0</pitch-deg>
        </offsets>
    </model>
    
     <model>
        <name>Light-cons43</name>
	<path>Aircraft/AS330-Puma/Models/Remb/Light_cons.xml</path>
