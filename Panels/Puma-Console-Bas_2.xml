<PropertyList>

    <path>Models/console-bas_2.ac</path>
    <texture-path>Models/Textures</texture-path>

    <animation>
            <condition>
            <equals>
                <property>sim/current-view/view-number</property>
                <value>999</value>
            </equals>
        </condition>
        <type>select</type>
        <object-name>Gabarit-Pose</object-name>
    </animation>
    
     <effect>
	<inherits-from>Aircraft/AS330-Puma/Models/Effects/reflectp-shine</inherits-from>	
	<object-name>ParkFreins-Cde</object-name>
	<object-name>SafeGear-Cde</object-name>
    </effect>
    
    <model>
