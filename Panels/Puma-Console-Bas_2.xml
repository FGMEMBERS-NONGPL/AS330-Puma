            <x-m>0.0</x-m>
            <y-m>0.16</y-m>
            <z-m>-0.02</z-m>	
	    <pitch-deg>0</pitch-deg>
        </offsets>
    </model>
    
    <model>
        <name>Light-cons23</name>
	<path>Aircraft/AS330-Puma/Models/Remb/Light_cons.xml</path>
        <offsets>
            <x-m>0.0</x-m>
            <y-m>-0.16</y-m>
            <z-m>-0.02</z-m>	
	    <pitch-deg>0</pitch-deg>
        </offsets>
    </model>
    
    
     <animation>
            <type>noshadow</type>
	    <object-name>Nav-Inst2</object-name>
    </animation>
    
    <effect>
	<inherits-from>Aircraft/AS330-Puma/Models/Effects/reflectp-shine</inherits-from>
	 <object-name>ParkFreins-Cde</object-name>
	 <object-name>SafeGear-Cde</object-name>
	 <!--<object-name>Nav-Inst2</object-name>-->
    </effect>
    
    
     <animation>
            <type>translate</type>
            <object-name>ParkFreins-Cde</object-name>
            <property>fdm/jsbsim/fcs/brake-parking-pos-norm</property>
            <factor>0.08</factor>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>1</z>
            </axis>
        </animation>

        <animation>
            <type>rotate</type>
            <object-name>ParkFreins-Cde</object-name>
            <property>fdm/jsbsim/fcs/brake-parking-pos-norm</property>
            <interpolation>
                <entry>
                    <ind>0.0</ind>
                    <dep>-90</dep>
                </entry>
                <entry>
                    <ind>0.4</ind>
                    <dep>-90</dep>
                </entry>
                <entry>
                    <ind>0.6</ind>
                    <dep>-90</dep>
                </entry>
                <entry>
                    <ind>1.0</ind>
                    <dep>0</dep>
                </entry>
            </interpolation>
            <center>
                <x-m>-0.179043</x-m>
                <y-m>0.156111</y-m>
                <z-m>0</z-m>
            </center>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>-1</z>
            </axis>
        </animation>
	
	
	<animation>
            <type>pick</type>
            <object-name>ParkFreins-Cde</object-name>
            <action>
                <button>0</button>
                <binding>
                    <command>property-toggle</command>
		    <property>fdm/jsbsim/fcs/brake-parking-cmd-norm</property>
                </binding>
            </action>
        </animation>
	
	
	
	


