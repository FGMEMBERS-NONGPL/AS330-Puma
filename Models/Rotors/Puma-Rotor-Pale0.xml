	<params>
		<flapD0>
			<factor>0.15</factor>
		</flapD0>
		<flapD1>
			<factor>0.45</factor>
		</flapD1>
		<flapD2>
			<factor>0.25</factor>
		</flapD2>
		<flapD3>
			<factor>0.10</factor>
		</flapD3>
		<flapD4>
			<factor>0.07</factor>
		</flapD4>
		<flap-deg>rotors/main/blade[0]/flap-deg</flap-deg>
		<blade-position>rotors/main/blade[0]/position-deg</blade-position>
	</params>

	<model>
            <!--<name>Rotor</name>-->
            <path>Aircraft/AS330-Puma/Models/Rotors/Puma-Rotor-Base.xml</path>
            <offsets>
                    <x-m>0</x-m>
                    <y-m>0</y-m>
                    <z-m>0</z-m>
                    <pitch-deg>0</pitch-deg>
            </offsets>
        </model>


	<animation>
		<condition>
			<less-than>
			<property>rotors/main/rpm</property>
			<value>210</value>
			</less-than>
		</condition>
		<type>select</type>
                <object-name>Rotor-PaleD1</object-name>
		<object-name>Rotor-PaleD2</object-name>
		<object-name>Rotor-PaleD3</object-name>
		<object-name>Rotor-PaleD4</object-name>
 	</animation>

        <animation>
		<condition>
			<greater-than>
				<property>rotors/main/rpm</property>
				<value>200</value>
			</greater-than>
		</condition>
		<type>select</type>
                <object-name>Rotor-FlouD0</object-name>
		<object-name>Rotor-FlouD1</object-name>
		<object-name>Rotor-FlouD2</object-name>
		<object-name>Rotor-FlouD3</object-name>
		<object-name>Rotor-FlouD4</object-name>
 	</animation>




	<!--=========================Idea from======BO105==========================================-->
	<!--==============ROTATE=======================-->


	<animation>
		<type>rotate</type>
		<object-name>Rotor-Tete-Cde</object-name>
                <object-name>Rotor-Tete-Fix</object-name>
		<property alias="../../params/blade-position"/>
		<offset-deg>0</offset-deg>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
	</animation>
        
        <animation>
            <type>rotate</type>
            
            <object-name>Rotor-Pale-Fix</object-name>
           
            <property alias="../../params/blade-position"/>
            <offset-deg>0</offset-deg>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>-1</z>
            </axis>
	</animation>

